<?xml version="1.0"?>
<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <!-- <param name="port" value="/dev/arduinoPWM"/> -->
        <param name="port" value="/dev/ttyACM0"/>
        <param name ="baud" value="250000" />
        <remap from="/lli/ctrl_request" to="/SVEA0/lli/ctrl_request" />
        <remap from="/lli/ctrl_actuated" to="/SVEA0/lli/ctrl_actuated" />
    </node>
    
    <node pkg="svea" type="lli_ctrl_to_twist.py" name="ctrl_to_twist" output="screen">
        <param name="ctrl_message_topic" value="/SVEA0/lli/ctrl_actuated" />
        <param name="twist_message_topic" value="/SVEA0/twist_from_control" />
        <param name="est_frame_id" value="base_link" />
        
    </node>

</launch>
